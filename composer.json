{
    "name": "encounterthecross/events-manager",
    "description": "This project is for Encounter The Cross, main website www.encounterthecross.com",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-zip": "*",
        "babdev/pagerfanta-bundle": "^4.2",
        "codeception/module-doctrine": "^3.1",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/doctrine-migrations-bundle": "^3.2|^4.0",
        "doctrine/orm": "^2.14|^3.0",
        "easycorp/easyadmin-bundle": "^4.6",
        "endroid/qr-code-bundle": "^5.0|^6.0",
        "guzzlehttp/psr7": "^2.6",
        "knplabs/knp-menu-bundle": "^3.2",
        "league/html-to-markdown": "^5.1",
        "liip/monitor-bundle": "^2.22",
        "pagerfanta/doctrine-orm-adapter": "^3.8.0|^4.0",
        "pagerfanta/twig": "^4.7",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.15",
        "scienta/doctrine-json-functions": "^4.3|^5.0|^6.0",
        "sentry/sentry-symfony": "^5.1",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/asset": "7.0.*",
        "symfony/asset-mapper": "7.0.*",
        "symfony/console": "7.0.*",
        "symfony/doctrine-messenger": "7.0.*",
        "symfony/dotenv": "7.0.*",
        "symfony/expression-language": "7.0.*",
        "symfony/flex": "^2",
        "symfony/form": "7.0.*",
        "symfony/framework-bundle": "7.0.*",
        "symfony/http-client": "7.0.*",
        "symfony/intl": "7.0.*",
        "symfony/mailer": "7.0.*",
        "symfony/mime": "7.0.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "7.0.*",
        "symfony/process": "7.0.*",
        "symfony/property-access": "7.0.*",
        "symfony/property-info": "7.0.*",
        "symfony/runtime": "7.0.*",
        "symfony/security-bundle": "7.0.*",
        "symfony/serializer": "7.0.*",
        "symfony/stimulus-bundle": "^2.12",
        "symfony/string": "7.0.*",
        "symfony/translation": "7.0.*",
        "symfony/twig-bundle": "7.0.*",
        "symfony/uid": "7.0.*",
        "symfony/ux-chartjs": "^2.22",
        "symfony/ux-live-component": "^2.22",
        "symfony/ux-turbo": "^2.10",
        "symfony/ux-twig-component": "^2.10",
        "symfony/validator": "7.0.*",
        "symfony/web-link": "7.0.*",
        "symfony/yaml": "7.0.*",
        "symfonycasts/reset-password-bundle": "^1.17",
        "symfonycasts/sass-bundle": "^0.8.2",
        "symfonycasts/tailwind-bundle": "^0.6.1",
        "tales-from-a-dev/flowbite-bundle": "^0.7.1",
        "twbs/bootstrap": "^5.3",
        "twig/cssinliner-extra": "^3.7",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/inky-extra": "^3.7",
        "twig/intl-extra": "^3.7",
        "twig/twig": "^2.12|^3.0",
        "tzunghaor/settings-bundle": "^0.15.0",
        "yectep/phpspreadsheet-bundle": "@dev"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true,
            "endroid/installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\Acceptance\\": "tests/acceptance",
            "App\\Tests\\Functional\\": "tests/functional",
            "App\\Tests\\Integration\\": "tests/integration",
            "App\\Tests\\Unit\\": "tests/unit",
            "App\\Tests\\": "tests/_support"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.0.*"
        }
    },
    "require-dev": {
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "^3.0",
        "codeception/module-db": "^3.1",
        "codeception/module-filesystem": "^3.0",
        "codeception/module-phpbrowser": "^3.0",
        "codeception/module-symfony": "^3.1",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.37",
        "php-parallel-lint/php-console-highlighter": "^1.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpmd/phpmd": "^2.14",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-symfony": "^1.3",
        "phpunit/phpunit": "^9.5|^10.0|^11.0",
        "symfony/browser-kit": "7.0.*",
        "symfony/css-selector": "7.0.*",
        "symfony/debug-bundle": "7.0.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/stopwatch": "7.0.*",
        "symfony/web-profiler-bundle": "7.0.*",
        "tavoniievez/module-foundry": "^2.0@dev",
        "zeek/zeek-build-process": "dev-main",
        "zenstruck/foundry": "^1.29|^2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:redpandacoding/build-process.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:jmwamser/module-foundry.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:redpandacoding/phpspreadsheet-bundle.git"
        }
    ]
}
