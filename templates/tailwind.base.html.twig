<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}{{ title|default("Men's Encounter") }}{% endblock %}</title>
    {% block icon_link %}<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">{% endblock %}
    {% block meta %}{% endblock %}

    {% block stylesheets %}
        {{ ux_controller_link_tags() }}
    {% endblock %}

    {% block javascripts %}
        <script
                src="https://js.sentry-cdn.com/348fef7ffde4e3763d97c39c5595ccd0.min.js"
                crossorigin="anonymous"
        ></script>
        {% block importmap %}
            {{ importmap('tailwind_app') }}
        {% endblock %}
    {% endblock %}
</head>
{% block body %}
    <body>
        {% block content %}
        {% endblock %}
        <twig:Taig:Modal>
            <turbo-frame
                    id="modal"
                    data-modal-target="dynamicContent"
                    class="aria-busy:opacity-50 transition-opacity"
            >
            </turbo-frame>
        </twig:Taig:Modal>
    </body>
{% endblock %}

</html>